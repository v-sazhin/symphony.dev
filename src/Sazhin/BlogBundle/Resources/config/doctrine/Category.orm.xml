<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Sazhin\BlogBundle\Entity\Category" table="category">
    <unique-constraints>
      <unique-constraint name="UNIQ_64C19C1989D9B62" columns="slug"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="127" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="slug" type="string" column="slug" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="post" target-entity="Post" mapped-by="category" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
